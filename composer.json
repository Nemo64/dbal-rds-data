    "description": "rds-data driver for doctrine dbal",
    "keywords": [
        "doctrine",
        "dbal",
        "driver",
        "rds",
        "aurora",
        "aws",
        "sdk",
        "data",
        "data-api"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marco Pfeiffer",
            "email": "git@marco.zone"
        }
    ],
    "require": {
