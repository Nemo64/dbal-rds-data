{
    "name": "nemo64/dbal-rds-data",
    "description": "rds-data driver for doctrine dbal",
    "keywords": [
        "doctrine",
        "dbal",
        "orm",
        "driver",
        "rds",
        "aurora",
        "serverless",
        "aws",
        "sdk",
        "data",
        "data-api",
        "symfony"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Marco Pfeiffer",
            "email": "git@marco.zone"
        }
    ],
    "require": {
        "php": "~7.2||~8.0",
        "aws/aws-sdk-php": "^3.98",
        "doctrine/dbal": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Nemo64\\DbalRdsData\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nemo64\\DbalRdsData\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "config": {
        "sort-packages": true
    }
}
